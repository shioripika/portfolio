		for (i = 0; i < x.length; i++) {
			x[i].style.display = "none";
		}
		for (i = 0; i < dots.length; i++) {
			dots[i].className = dots[i].className.replace(" current-dot", "");
		}
		x[slideIndex - 1].style.display = "block";
		dots[slideIndex - 1].className += " current-dot";
	}

	function vote() {
		var photoId = $('.mySlides').eq(slideIndex - 1).attr('photoId');
		$.ajax({
			"url": "/ajax/public/vote_photo?photo_id=" + photoId + "&point=" + point,
			"method": "GET",
			success: function(response) {
				console.log(response);
			}
		});
	}

	function download() {
		$('[data-remodal-id=photo-vote]').remodal().close();
		showDivs(1);

		// Download
		$('#thumbnail_overlay_area').fadeIn();
		var timerModal = $('[data-remodal-id=timer-modal]').remodal();
		if (timerModal.getState() == 'opened' || timerModal.getState() == 'opening') {
			timerModal.close();
		}
		$("#jq_dljump_url")[0].click();
		// End Download
	}

	$(document).ready(function() {
		$(document).on('opening', '.remodal.photo-vote', function() {
			$('#voting-taboola-wrapper').html('<div id="taboola-below-voting-area"></div>');
			$.ajax({
				"url": "/ajax/public/get_voting_photo",
				"method": "GET",
				success: function(response) {
					imgHtml = '';
					photos = response.data.items;
					photoCount = photos.length;
					if (photoCount < 5) {
						//download();
						$('#timer').hide();
						$('#download').show();
						$('[data-remodal-id=photo-vote]').remodal().close();
						return;
					}
					photos.forEach(function(element) {
						imgHtml += '<img photoId="' + element.id + '" class="mySlides" src="https://d1f5hsy4d47upe.cloudfront.net/' + element.filename.substring(0, 2) + '/' + element.filename + '_w.jpg">';
					});
					$('.w3-content').html(imgHtml);
					plusDivs(1 - slideIndex);
					$('#labelForStar0').click() // Reset stars
				}
			});
		});

		$(document).on('opened', '.remodal.photo-vote', function() {
			window._taboola = window._taboola || [];
			_taboola.push({
				mode: 'thumbnails-b',
				container: 'taboola-below-voting-area',
				placement: 'Below Article Thumbnails Video',
				target_type: 'mix'
			});
		});

		$('#photo-vote span#next-image').click(function() {
			if ($(this).hasClass('grey'))
				return;
			else if (slideIndex == 5) { // Done Voting
				vote();
				$('#timer').hide();
				$('#download').show();
				$('[data-remodal-id=photo-vote]').remodal().close();
				//download();

			} else {
				vote();
				$('#labelForStar0').click() // Reset stars
				$('#photo-vote span#next-image').addClass('grey');
				plusDivs(1);
			}
		});

		$('.rate label').click(function() {
			point = $(this).attr('point');
			if (point > 0)
				$('#photo-vote span#next-image').removeClass('grey');
		});
	});
